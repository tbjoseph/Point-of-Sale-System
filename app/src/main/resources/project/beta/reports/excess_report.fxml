<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.beta.reports.ExcessReportController">
    <children>
        <Label text="Restock Report" />
        <TableView fx:id="inventoryTable" layoutX="26.0" layoutY="34.0">
            <columns>
                <TableColumn fx:id="itemNameCol" prefWidth="200.0" text="Item Name" />
                <TableColumn fx:id="currentInventoryCol" prefWidth="150.0" text="Inventory at Timestamp" />
                <TableColumn fx:id="previousInventoryCol" prefWidth="150.0" text="Current Inventory" />
            </columns>
        </TableView>
        <Button text="Back" onAction="#goBack" />
    </children>
</VBox>